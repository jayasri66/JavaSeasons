package polymorphism;

public class Assignment 
{
 String panCard,voterId,passport;

	public void Aadhar(String panCard)
	{
	System.out.println("Aadhar number with " + panCard + " is " + (int)(Math.random()*10000) + " "
	+(int)(Math.random()*10000) + " "+ (int)(Math.random()*10000) + " ");
	}
	public void Aadhar(String panCard, String voterId)
	{
	System.out.println("Aadhar number with " + panCard +" "+ voterId  + " is " + (int)(Math.random()*10000) + " "
	+(int)(Math.random()*10000) + " " + (int)(Math.random()*10000) + " ");
    }
	public void Aadhar(String panCard, String voterId, String passport)
	{
		System.out.print("Aadhar number with " + panCard +" "+voterId +" " + passport  + " is " + (int)(Math.random()*10000) + " " + (int)(Math.random()*10000) + " " + (int)(Math.random()*10000) + " ");
	}
	
}
	
