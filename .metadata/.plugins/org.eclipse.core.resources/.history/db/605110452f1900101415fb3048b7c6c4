package homework;

public class EmployeeData1 {
	int mobileNumber;
	int empId;
	String email;
	String empName;
	public Employee1(int mobileNumber,int empId,String email,String empName)
	{
		this.mobileNumber = mobileNumber;
        this.empId = empId;
        this.email = email;
        this.empName = empName;	
	}

}
