package homework;

public class Employee extends EmployeeData {

	public static void main(String[] args) {
		
		Employee obj= new Employee();
		obj.employeeDetails(123, "12356@gmail.com", "asdf", 1234567891);

	}

	public void employeeDetails(int empId,String email,String empName, int mobileNumber)
	{
	System.out.println("Employee Name" + empName );
	System.out.println("Employee Name" + empId );
	System.out.println("Employee Name" + email );
	System.out.println("Employee Name" + mobileNumber );
	}
}
